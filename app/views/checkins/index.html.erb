<h1>Listing checkins</h1>

<table>
  <thead>
    <tr>
      <th>Checkin</th>
      <th>Created at</th>
      <th>Rating score</th>
      <th>Uid</th>
      <th>Bid</th>
      <th>Brewery</th>
      <th>Venue city</th>
      <th>Venue state</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checkins.each do |checkin| %>
      <tr>
        <td><%= checkin.checkin_id %></td>
        <td><%= checkin.created_at %></td>
        <td><%= checkin.rating_score %></td>
        <td><%= checkin.uid %></td>
        <td><%= checkin.bid %></td>
        <td><%= checkin.brewery_id %></td>
        <td><%= checkin.venue_city %></td>
        <td><%= checkin.venue_state %></td>
        <td><%= checkin.lat %></td>
        <td><%= checkin.lng %></td>
        <td><%= link_to 'Show', checkin %></td>
        <td><%= link_to 'Edit', edit_checkin_path(checkin) %></td>
        <td><%= link_to 'Destroy', checkin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checkin', new_checkin_path %>
